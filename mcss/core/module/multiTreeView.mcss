.m-multitreeview {
    .treeview_list {
        margin-left: 3.4em;
    }

    .treeview_item {
        > .u-icon {
            left: -3em;
        }

        > .u-check2 {
            position: absolute;
            left: -1.8em;
            padding: 3px 0;
        }
    }
}